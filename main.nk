atom check_if_bigger_than(a: i32, b: i32): i32 {
	
	pure r: i32 = 0;

	if a > b { r += 10; };

	if a < 30 {
		if a != 16 { r += 5; };
		r += 3;
	};

	return load r;
}

fn main(): i32 {
	
	pure a1: i32 = 15;
	pure a2: i32 = 6;

	return load check_if_bigger_than(a1, a2);
}